<html>
    <head>
        <% include ../partials/head %>            
    </head>
    <body>              
        <div class="container">            
            <table class="table table-hover">
                <thead>
                    <tr>
                        <th>#</th>
                        <th>Nome</th>
                        <th>Email</th>                        
                    </tr>
                </thead>
                <tbody>
                    <% usuarios.forEach((usuario) => { %>
                        <tr>
                            <th scope="row"><a href=<%= "/usuario/cadastro/".concat(usuario._id) %>>Editar</a></th>
                            <td><%= usuario.nome %></td>
                            <td><%= usuario.email %></td>
                        </tr>
                    <%});%>                                        
                </tbody>
            </table>         
        </div>
        <% include ../partials/footer %>        
    </body>
</html>